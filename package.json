{
  "name": "pg-role",
  "version": "0.0.7",
  "description": "opinionated pg abstraction",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "docs-serve": "documentation serve index.js --watch --theme node_modules/clean-documentation-theme",
    "docs-build": "documentation build index.js -f html -o docs --theme node_modules/clean-documentation-theme",
    "postdocs-build": "documentation readme index.js -s Methods --theme node_modules/clean-documentation-theme",
    "test": "clear && mocha --exit --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vertebrae-org/pg-role.git"
  },
  "keywords": [
    "pg",
    "connection",
    "pools"
  ],
  "author": "Rob Higgins <rob@vertebrae.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/vertebrae-org/pg-role/issues"
  },
  "homepage": "https://github.com/vertebrae-org/pg-role#readme",
  "dependencies": {
    "dotenv": "^6.0.0",
    "pg": "^7.4.3",
    "pg-escape": "^0.2.0"
  },
  "devDependencies": {
    "clean-documentation-theme": "^0.5.2",
    "documentation": "^8.1.2",
    "mocha": "^5.2.0"
  }
}
